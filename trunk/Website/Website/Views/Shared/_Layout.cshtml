<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    
    <link href="@Url.Content("~/css/Site.css")" rel="stylesheet" type="text/css" />
    <link  href="http://fonts.googleapis.com/css?family=Vollkorn:regular,italic,bold,bolditalic" rel="stylesheet" type="text/css" >
    <link  href="http://fonts.googleapis.com/css?family=Quattrocento+Sans:regular" rel="stylesheet" type="text/css" >
    
    <script src="@Url.Content("~/js/lib/jquery-1.4.1.min.js")" type="text/javascript"></script>
    <!--script type="text/javascript" src="/ckeditor/ckeditor.js"></script>
    <script type="text/javascript" src="/ckeditor/adapters/jquery.js"></script>
    <script type="text/javascript" src="/js/HtmlContent.js"></script-->
</head>

<body>
    <div id="container">
        <div id="sidebar">
            <div id="header">
                <h1><a href="/">Stu Sherwin</a></h1>
                @Html.Partial("_Tagline")
            </div>
            @Html.Partial("_Navigation")
        </div>
        <div id="content">
            <div class="margin-fix">
                @RenderBody()
            </div>
        </div>
        @Html.Partial("_Contact")
        <div class="shadow-left"></div>
        <div class="shadow-right"></div>
        @Html.Partial("_Scroll")
    </div>
    <script type="text/javascript" src="/js/Scrolling.js"></script>
</body>
</html>
